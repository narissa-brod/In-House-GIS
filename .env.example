VITE_AIRTABLE_TOKEN=YOUR_AIRTABLE_TOKEN
VITE_AIRTABLE_BASE=appsWI39rrzGmC2RH
VITE_AIRTABLE_TABLE=Target Land Acquisitions
VITE_AIRTABLE_VIEW=LinkedToMap
VITE_AIRTABLE_TABLE_ID=tblXXXXXXXXXXXXXX
VITE_GOOGLE_MAPS_KEY=YOUR_GOOGLE_MAPS_KEY

# Vector tiles (XYZ MVT) URL for parcels layer
# Option 1: Use Supabase Edge Function (recommended) - replace with your actual Supabase URL
# VITE_PARCELS_TILES_URL=https://your-project.supabase.co/functions/v1/parcels-tile?z={z}&x={x}&y={y}
# Option 2: Local tiles: /tiles/{z}/{x}/{y}.pbf (served from public/tiles)
# Option 3: CDN: https://cdn.example.com/tiles/parcels_v1/{z}/{x}/{y}.pbf
VITE_PARCELS_TILES_URL=/tiles/{z}/{x}/{y}.pbf
VITE_PARCELS_TILES_MIN_ZOOM=15
VITE_PARCELS_GEOJSON_MIN_ZOOM=18

# Optional: Kaysville General Plan vector tiles URL (XYZ MVT)
# If set, the app will render the GP layer as tiles instead of static GeoJSON for better performance
# Example: https://cdn.example.com/tiles/kaysville_gp_v1/{z}/{x}/{y}.pbf
VITE_GP_TILES_URL=
VITE_GP_TILES_MIN_ZOOM=10
VITE_GP_TILES_MAX_ZOOM=22

# Optional: Static General Plan GeoJSON fallback served from /public
# Used when VITE_GP_TILES_URL is not provided
VITE_GP_STATIC_URL=/gp/general_plan_kaysville.geojson

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Supabase Service Role Key (for server-side operations like bulk imports)
# WARNING: Keep this secret! Never commit to git or expose to client
SUPABASE_SERVICE_KEY=your_supabase_service_role_key
